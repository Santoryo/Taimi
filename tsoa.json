{
    "entryFile": "src/app.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["src/routes/**/*controller.ts"],
    "spec": {
        "outputDirectory": "build",
        "specVersion": 3,
        "securityDefinitions": {
            "supabase": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "routes": {
        "routesDir": "build",
        "authenticationModule": "./src/authentication.ts"
    }
}
